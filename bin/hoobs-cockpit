#!/usr/bin/env node

process.title = "hoobs-cockpit";

const File = require("fs");
const Path = require("path");

new (require(Path.join(Path.dirname(File.realpathSync(__filename)), "../lib/cockpit")))().start(true).then((registration) => {
    console.log("registration code");
    console.log(registration);
}).catch((error) => {
    console.log(error);
});
