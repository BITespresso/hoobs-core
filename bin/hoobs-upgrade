#!/usr/bin/env node

process.title = "hoobs-upgrade";

const File = require("fs");
const Path = require("path");

(async () => {
    await (require(Path.join(Path.dirname(File.realpathSync(__filename)), "../scripts/nginx")))(false);
    await (require(Path.join(Path.dirname(File.realpathSync(__filename)), "../scripts/config")))();
})();
